					"delimiter": "",
					"eol": "",
					"quote_char": "\"",
					"head": true,
					"skip_empty_lines": true,
					"dynamic_typing": true,
					"pluck": true,
					"embedding_object": false,
					"nb_meta_lines": 0,
					"nb_subheader_lines": 0
				}
			},
