  "scripts": {
    "build": "npm run build:prod",
    "build:clean": "rimraf build reports",
    "build:dev": "npm run build:clean && npm run build:mkdocs && npm run build:dev:css+js+html",
    "build:dev:css+js+html": "webpack --config=configs/webpack.dev.js",
    "build:dev:css+js+html+lint": "webpack --config=configs/webpack.dev.js --env enableEslintPlugin",
    "build:prod": "npm run build:clean && npm run build:mkdocs && npm run build:prod:css+js+html && node configs/imagemin.mjs",
    "build:prod:css+js+html": "webpack --config=configs/webpack.prod.js",
    "lint:analyzer": "webpack --config=configs/webpack.analyzer.js",
    "lint:eslint": "eslint source/",
    "lint:eslint:report": "eslint -f html -o reports/jshint.html source/",
    "lint:fix:prettier": "prettier --ignore-path .prettierignore --write .",
    "lint:prettier": "prettier --ignore-path .prettierignore --check .",
    "start": "concurrently \"npm run start:dev\" \"npm run start:dev:flask\"",
    "start:dev": "npm run build:mkdocs && npm run start:dev:watch",
    "start:dev:flask": "python ../main.py --dev --port=5000",
    "start:dev:watch": "webpack-cli serve --config=configs/webpack.dev.js",
    "build:mkdocs": "mkdocs build -c -f \"../documentation/mkdocs.yml\" && node configs/replaceDocConf.mjs",
    "test": "npm run test:mocha",
    "test:mocha": "mochapack --require source/test-init.mjs --webpack-env --webpack-config configs/webpack.test.js \"source/**/*.spec.js\""
  }
